<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<!--category를 다루는 mapper  -->
	<mapper namespace="category">
		<select id="so" resultType="SoDTO">
			SELECT *
			FROM so
		</select>
		<select id="dae" resultType="DaeDTO">
			SELECT *
			FROM dae
		</select>
	</mapper>
